<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shahrir.nytimes_articles_search">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.shahrir.nytimes_articles_search.nytimes_saved_articles_webview"></activity>

        <activity android:name="com.shahrir.nytimes_articles_search.nytimes_articles_expanded" />

        <activity
            android:name="com.shahrir.nytimes_articles_search.nytimes_articles_saved"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name="com.shahrir.nytimes_articles_search.nytimes_articles_activity"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name="com.shahrir.nytimes_articles_search.nytimes_main"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name="com.shahrir.nytimes_articles_search.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>



    </application>

</manifest>